<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:padding="@dimen/_10sdp">


    <LinearLayout
        android:id="@+id/iv_viewPager_data_time"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_gravity="left"
        android:layout_marginLeft="@dimen/_15sdp"
        android:background="@drawable/ic_rectangle_date"
        android:orientation="horizontal"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.012"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.024">

        <TextView
            android:id="@+id/vp_data_time_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/_6sdp"
            android:fontFamily="@font/montserrat_bold"
            android:text="19:00"
            android:textColor="@color/activeBottomNavIconColor"
            android:textSize="@dimen/_14ssp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/iv_viewPager_data_time3"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_gravity="center_horizontal"
        android:layout_marginRight="@dimen/_10sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:background="@drawable/ic_rectangle_date">
        <TextView
            android:id="@+id/vp_data_time_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/_6sdp"
            android:fontFamily="@font/montserrat_bold"
            android:text="21:00"
            android:textColor="@color/activeBottomNavIconColor"
            android:textSize="@dimen/_15ssp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/iv_viewPager_data_time2"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginLeft="@dimen/_70sdp"
        android:layout_marginRight="@dimen/_10sdp"
        android:background="@drawable/ic_rectangle_date">
        <TextView
            android:id="@+id/vp_data_time_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_6sdp"
            android:fontFamily="@font/montserrat_bold"
            android:text="20:00"
            android:textColor="@color/activeBottomNavIconColor"
            android:textSize="@dimen/_14ssp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/iv_viewPager_data_time4"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_gravity="right"
        android:layout_marginRight="@dimen/_70sdp"
        android:layout_marginLeft="@dimen/_10sdp"
        android:background="@drawable/ic_rectangle_date">
        <TextView
            android:id="@+id/vp_data_time_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_6sdp"
            android:fontFamily="@font/montserrat_bold"
            android:text="22:00"
            android:textColor="@color/activeBottomNavIconColor"
            android:textSize="@dimen/_14ssp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/iv_viewPager_data_time5"
        android:layout_width="@dimen/_50sdp"
        android:layout_height="@dimen/_50sdp"
        android:layout_marginRight="@dimen/_15sdp"
        android:layout_gravity="right"
        android:background="@drawable/ic_rectangle_date">
        <TextView
            android:id="@+id/vp_data_time_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_6sdp"
            android:fontFamily="@font/montserrat_bold"
            android:text="23:00"
            android:textColor="@color/activeBottomNavIconColor"
            android:textSize="@dimen/_14ssp" />
    </LinearLayout>


</androidx.cardview.widget.CardView>